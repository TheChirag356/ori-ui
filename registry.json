{
	"$schema": "https://shadcn-svelte.com/schema/registry.json",
	"name": "Ori UI",
	"homepage": "https://ori-ui.vercel.app",
	"aliases": {
		"lib": "$lib",
		"components": "$lib/components",
		"utils": "$lib/utils/utils",
		"ui": "$lib/components/ui",
		"hooks": "$lib/hooks"
	},
	"items": [
		{
			"name": "shimmer-button",
			"type": "registry:block",
			"title": "Shimmer Button",
			"description": "A customizable shimmer button component for Svelte applications.",
			"registryDependencies": ["button"],
			"files": [
				{
					"path": "src/lib/registry/buttons/shimmer-button/shimmer-button.svelte",
					"type": "registry:file",
					"target": "ori/shimmer-button/shimmer-button.svelte"
				},
				{
					"path": "src/lib/registry/buttons/shimmer-button/index.ts",
					"type": "registry:file",
					"target": "ori/shimmer-button/index.ts"
				}
			],
			"author": "Chirag Arora <arorachirag12005@gmail.com>",
			"meta": {
				"author": "TheChirag356",
				"version": "1.0.0",
				"documentation": "https://ori-ui.vercel.app/components/shimmer-button",
				"repository": "https://github.com/TheChirag356/ori-ui"
			}
		},
		{
			"name": "slide-text-button",
			"type": "registry:block",
			"title": "Slide Text Button",
			"description": "An animated button with vertical slide text transition effect",
			"registryDependencies": ["button"],
			"files": [
				{
					"path": "src/lib/registry/buttons/slide-text-button/slide-text-button.svelte",
					"type": "registry:file",
					"target": "ori/slide-text-button/slide-text-button.svelte"
				},
				{
					"path": "src/lib/registry/buttons/slide-text-button/index.ts",
					"type": "registry:file",
					"target": "ori/slide-text-button/index.ts"
				}
			],
			"author": "Chirag Arora <arorachirag12005@gmail.com>",
			"meta": {
				"author": "TheChirag356",
				"version": "1.0.0",
				"documentation": "https://ori-ui.vercel.app/components/shimmer-button",
				"repository": "https://github.com/TheChirag356/ori-ui"
			}
		}
	]
}
